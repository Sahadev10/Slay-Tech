{
  "manifest_version": 2,
  "name": "Image Grabber",
  "version": "1.0",
  "description": "Grabs images and sends them to backend",
  "permissions": [
    "activeTab",
    "http://localhost/*",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://localhost/*"
      ],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
